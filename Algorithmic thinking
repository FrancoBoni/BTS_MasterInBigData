def look_year(year):
#function to get the list of the week day average traffic intensity
    return traffic_data.get(year)

value = look_year("2016")
value

def get_min():
#function to get the minimun value of that list
    return min(value)
    
minimum = get_min()
minimum


store = []
def store_or_notsaved():
#function to know if the value will be stored or not
    if minimum > 2000:
        store.append(minimum)
    else: print("data nos saved")
        
action = store_or_notsaved()
action
print(store)
